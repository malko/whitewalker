
	<li id="{{name}}" class="status-{{status}}">
		<div>
			<span class="name">{{name}}</span>
			<span class="datetime">last run at: {{status|!=running?}}{{endTime}}{{?status}}{{status|=running?}}{{startTime}}{{?status}}</span>
			<span class="duration">in {{duration}}</span>
			<button type="buttton" class="testrunner" rel="{{name}}">run test</button>
		</div>
		<pre>{{out}}</pre>
	</li>
