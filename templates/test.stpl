
	<dt id="{{test.name}}" class="status-{{test.status}}">
		<div>
			<span class="name">{{test.name}}</span>
			<span class="datetime">last run at:
			{{test.status|!=running?}}{{test.endTime}}{{?test.status}}
			{{test.status|=running?}}{{test.startTime}} (start at){{?test.status}}</span>
			<span class="duration">in {{test.duration}}</span>
			<div class="actions">{{>test-run-buttons}}</div>
		</div>
	</dt>
	<dd id="{{test.name}}-dd">
		<pre>{{test.out}}</pre>
	</dd>
